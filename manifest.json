{
  "manifest_version": 3,
  "name": "MinusAi",
  "version": "1.0",
  "description": "Appends -ai flag to google search",
  "permissions": [
    "webRequest",
    "scripting",
    "webRequestBlocking"
],
  "host_permissions": ["https://www.google.com/*"],
  "background": {
    "service_worker": "background.js"
  }
}
