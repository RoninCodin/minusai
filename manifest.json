{
  "manifest_version": 3,
  "name": "Google -AI Appender",
  "version": "1.0",
  "description": "Automatically adds -ai to Google searches.",
  "permissions": ["scripting"],
  "host_permissions": ["https://www.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
